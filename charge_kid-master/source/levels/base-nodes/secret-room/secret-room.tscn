code = "shader_type canvas_item;

uniform int reveal;

void fragment() {
	if (reveal == 1) {
		vec2 coords = floor(UV/TEXTURE_PIXEL_SIZE);
		float sum = mod(coords.x + coords.y, 2);
		COLOR.a = sum;
		COLOR.rgb = texture(TEXTURE, UV).rgb;
	}
	if (reveal == 2) {
		COLOR.a = 0.0;
	}
	if (reveal == 0) {
		COLOR = texture(TEXTURE, UV);
	}
}

"

